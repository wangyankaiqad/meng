/* pages/wodeke/wodeke.wxss */
.header {
    padding: 0 40rpx;
    font-size: 60rpx;
    font-weight: 900;
}

.nav {
    display: flex;
    justify-content: space-evenly;
    margin-top: 20rpx;
}
.active{
    background-color: #eb4450;
    padding: 15rpx;
    border-radius: 50%;
    color: #fff;
}
.aa{
    display: flex;
    margin: 0 20rpx;
    justify-content: space-between;
    align-items: center;
    background-color: rgb(204, 204, 204,0.1);
    padding: 20rpx;
}
.right .img{
    width: 50rpx;
    height: 50rpx;
}
.img image{
    width: 100%;
    height: 100%;
}
.left .t1{
    color: #eb4450;
}
.left .t2{
    font-size: 12px;
}
.right{
    font-size: 12px;
    color:gray;
}
.bb{
    display: flex;
    margin-top: 100rpx;
}
.b1{
    flex: 1;
    text-align: center;
}
.b1 .b2{
    width: 100rpx;
    height: 100rpx;
    margin: auto;
}
.b2 image{
    width: 100%;
    height: 100%;
}
.b3{
    font-size: 12px;
}
.cc{
margin-top: 100rpx;
text-align: center;
}
.c1{
    width: 300rpx;
    height: 300rpx;
    margin: auto;
}
.c1 image{
    width: 100%;
    height: 100%;
}
.c2{
    font-size: 12px;
    color:gray;
    margin-top: 10rpx;
}

.btn{
    margin: 0 40rpx;
    background-color: rgb(241, 90, 36);
    border-radius: 40rpx;
    margin-top: 50rpx;
    color: #fff;
}

.dd{
    height: 100px;
}