    overflow: hidden;
    margin-top: 60rpx;

}

.a .a3 {
    width: 60%;
    height: 60rpx;
    background-color: #eb4450;
    text-align: center;
    line-height: 60rpx;
    margin: auto;
    border-radius: 30rpx;
    color: #fff;
    font-size: 28rpx;
}

.a .a4 {
    font-size: 24rpx;
    margin-top: 10rpx;
    color: #eb4450;
    margin-bottom: 20rpx;
}

