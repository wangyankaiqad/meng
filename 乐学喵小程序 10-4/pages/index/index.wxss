.header {
    display: flex;
    /* padding: 0 40rpx; */
    justify-content: space-between;
    height: 80rpx;
    align-items: center;
}

.header .right image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 40rpx;
}

.header .left {
    font-weight: 900;
}

.daohang {
    display: flex;
    justify-content: space-around;
}

.daohang .doa1 {
    padding: 10rpx;

}

.active {
    border-bottom: 4rpx solid #eb4450;
    color: #eb4450;
}

.color {
    color: red;
    border-bottom: 1px solid red;
}

.swiper-class {
    padding: 0 30rpx;
    margin-top: 30rpx;
}


swiper-item image {
    border-radius: 40rpx;
    height: 100%;
    width: 100%;
}

.top {
    text-align: center;
    margin-top: 40rpx;
}

.top view {
    color: gray;
}

.top image {
    width: 100rpx;
    height: 50rpx;
}

.middle {
    display: flex;
    justify-content: space-between;
    padding: 0 30rpx;
    align-items: center;
}

.middle .left {
    font-weight: 800;
    font-size: 40rpx;
}

.middle .right {
    font-size: 24rpx;
}

.a {
    width: 30%;
    text-align: center;
    background-color: rgb(128, 128, 128, .3);
    position: relative;
    margin-top: 80rpx;
}

.a .a1 {
    width: 100rpx;
    height: 100rpx;
    position: absolute;
    top: -50rpx;
    left: 55rpx;
}

.a .a1 image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.footer {
    display: flex;
